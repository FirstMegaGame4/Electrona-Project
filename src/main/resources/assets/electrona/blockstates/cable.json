{
  "multipart": [
    {
      "when": {
        "middle": "close"
      },
      "apply": {
        "model": "electrona:block/cable_mid"
      }
    },
    {
      "when": {
        "middle": "open"
      },
      "apply": {
        "model": "electrona:block/cable_mid_uninsulated"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "electrona:block/cable_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "electrona:block/cable_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "electrona:block/cable_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "electrona:block/cable_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "electrona:block/cable_up"
      }
    }
  ]
}